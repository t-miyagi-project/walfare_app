{
	"name": "Django Sample",
	// Dockerfileでイメージ・コンテナを作成
	"dockerComposeFile": "../docker-compose.yml",
	// VS Codeのワークスペースフォルダ
	"workspaceFolder": "/workspace",
	// 使用するサービス
	"service": "web",
	// リモート先のVS Codeにインストールする拡張機能\r
	"extensions": [
		"ms-python.python",
		"eamodio.gitlens",
		"mhutchie.git-graph",
		"shd101wyy.markdown-preview-enhanced"
	],
	"shutdownAction": "stopCompose"
}